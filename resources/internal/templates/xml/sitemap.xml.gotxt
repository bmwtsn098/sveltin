<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	{{- $baseURL := .NoPageData.Config.BaseURL -}}
	{{- $changeFreq := .NoPageData.Config.SitemapChangeFreq -}}
	{{- $priority := .NoPageData.Config.SitemapPriority -}}
	{{- $resources := .NoPageData.Items.Resources -}}
	{{ $contentValues := .NoPageData.Items.Content }}
	<url>
		<loc>{{ $baseURL }}</loc>
		<changefreq>{{ $changeFreq }}</changefreq>
		<priority>{{ $priority }}</priority>
	</url>
{{- range $resource := $resources -}}
{{ if $resource }}
	<url>
		<loc>{{ $baseURL }}/{{ $resource }}/</loc>
		<changefreq>{{ $changeFreq }}</changefreq>
		<priority>{{ $priority }}</priority>
	</url>
		{{ range $content := index $contentValues $resource }}
	<url>
		<loc>{{ $baseURL }}/{{ $resource }}/{{ $content }}/</loc>
		<changefreq>{{ $changeFreq }}</changefreq>
		<priority>{{ $priority }}</priority>
	</url>
		{{ end }}
{{ end }}
{{- end -}}
</urlset>
