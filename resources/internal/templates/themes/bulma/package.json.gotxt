{
	"name": "{{ .ProjectName }}",
	"version": "0.1.0",
	"packageManager": "{{ .NPMClient }}",
	"scripts": {
		"dev": "vite dev",
		"clean": "rimraf build",
		"build": "npm run clean && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@indaco/svelte-iconoir": "^2.5.1",
		"@sveltejs/adapter-static": "1.0.0-next.35",
		"@sveltejs/kit": "1.0.0-next.370",
		"@sveltinio/essentials": "^0.2.1",
		"@sveltinio/media-content": "^0.3.2",
		"@sveltinio/seo": "^0.1.7",
		"@sveltinio/services": "^0.3.1",
		"@sveltinio/widgets": "^0.1.7",
		"@types/gtag.js": "^0.0.10",
		"@types/lodash-es": "^4.17.6",
		"@typescript-eslint/eslint-plugin": "^5.30.5",
		"@typescript-eslint/parser": "^5.30.5",
		"bulma": "^0.9.3",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"install": "^0.13.0",
		"lodash-es": "^4.17.21",
		"mdast-util-to-string": "^3.1.0",
		"mdsvex": "^0.10.6",
		"mdsvex-relative-images": "^1.0.2",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-slug": "^5.0.1",
		"remark-emoji": "^3.0.2",
		"remark-external-links": "9.0.1",
		"remark-preview": "^1.0.2",
		"remark-reading-time": "1.0.1",
		"remark-slug": "^7.0.1",
		"rimraf": "^3.0.2",
		"sass": "^1.53.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.0",
